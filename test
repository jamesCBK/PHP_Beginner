#include <stdio.h>
int main()
{
    int fact_func,fact_input,fact_output=1; // declaration
    scanf("%d",&fact_input); //5
    fact_func = fact_function(fact_output,fact_input);
    printf("%d",fact_func);
    return 0; // program end //return fact_output
}

int fact_function(int fact_output,int fact_input){
    for(int index=fact_input;index>=1;index--){
        fact_output *= index;
    }
return fact_output;
}




